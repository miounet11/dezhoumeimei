{
  "name": "pokeriq-pro",
  "version": "1.0.4",
  "description": "Professional Texas Hold'em Poker Training Platform with AI and GTO Strategy",
  "author": "PokerIQ Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' next dev -p 8820",
    "dev:test": "NODE_OPTIONS='--max-old-space-size=8192' next dev -p 8821",
    "dev:staging": "NODE_OPTIONS='--max-old-space-size=8192' next dev -p 8822",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:prod": "NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=production next build",
    "build:electron": "electron-builder",
    "build:mobile": "capacitor build",
    "start": "next start -p 8830",
    "start:prod": "NODE_ENV=production node .next/standalone/server.js",
    "start:standard": "NODE_ENV=production next start -p 8831",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-scenarios": "tsx prisma/seed-scenarios.ts",
    "db:seed-test-users": "tsx prisma/seed-test-users.ts",
    "db:seed-game-sessions": "tsx prisma/seed-game-sessions.ts",
    "db:studio": "prisma studio --port 8840",
    "socket-server": "node socket-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run socket-server\"",
    "deploy:vercel": "vercel --prod",
    "analyze": "ANALYZE=true next build",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --watch",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --coverage",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --testPathPattern=__tests__/integration",
    "test:a11y": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --testPathPattern=__tests__/accessibility",
    "test:performance": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --testPathPattern=__tests__/performance",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=4096' FORCE_COLOR=0 jest --testPathPattern=__tests__/(?!integration|accessibility|performance)",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:lighthouse": "lhci autorun --config=lighthouse.config.js",
    "test:lighthouse:desktop": "lhci autorun --config=lighthouse.config.js --preset=desktop",
    "test:security": "npm audit --audit-level=high && npm run audit:licenses",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:a11y && npm run test:e2e && npm run test:lighthouse",
    "test:ci": "npm run test:coverage && npm run test:integration && npm run test:a11y",
    "audit:licenses": "npm ls --prod --parseable | grep node_modules | xargs -I {} sh -c 'echo \"{}\" && cat \"{}/package.json\" | jq -r \".license // \\\"UNKNOWN\\\"\"' | paste - -",
    "playwright:install": "playwright install --with-deps",
    "playwright:codegen": "playwright codegen localhost:8820"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.13.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/nextjs": "^10.3.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "antd": "^5.26.7",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.537.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "pino": "^9.8.0",
    "pino-logflare": "^0.5.2",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-redux": "^9.2.0",
    "recharts": "^3.1.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "axe-core": "^4.10.3",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.8.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
