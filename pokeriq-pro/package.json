    "@types/ioredis": "^4.28.10",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "antd": "^5.26.7",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
